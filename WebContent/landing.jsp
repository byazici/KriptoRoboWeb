<script>
	
	 (function($){
	        function processForm( e ){
	            $.ajax({
	                url: 'feedback_.jsp',
	                dataType: 'json',
	                type: 'post',
	                data: $(this).serialize(),
	                success: function( data, textStatus, jQxhr ){
	                	 	$("#fbResult").removeClass("bg-primary");
	                	
	                		respCode = data['respCode'];
	                		respMsg = data['respMsg'];
	                		
		                	if (respCode == 1){	
		                		$("#fbForm").hide();
		                		$("#fbResult").html(respMsg);
		                	} else {
		                	 	$("#fbResult").addClass("bg-primary");
		                		$("#fbResult").html(respMsg);
		                	}
	                },
	                error: function( jqXhr, textStatus, errorThrown ){
	                }
	            });

	            e.preventDefault();
	        }

	        $('#fbForm').submit( processForm );
	    })(jQuery);
	
	
	</script>		
        
